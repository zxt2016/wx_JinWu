.page-section{
  margin-top: 20rpx;
}
.swiper_img{
  border-radius: 20rpx;
  overflow: hidden;
}
.swiper_img image{
  border-radius: 20rpx;
}
/* 电站招募 */
.stationBox{
  margin-top: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.stationBox .tabMain{
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 0;
  overflow: hidden;
}
.stationBox .tabMain .tabItem{
  width: 50%;
  font-size: 28rpx;
  padding: 30rpx 0;
}
.stationBox .tabMain .tabItem .item_view{
  display: flex;
  align-items: center;
  justify-content: center;
}
.stationBox .tabMain .tabItem .item_view .left_icon{
  display: block;
  height: 36rpx;
  width: 24rpx;
}
.stationBox .tabMain .tabItem .item_view .right_icon{
  display: block;
  height: 18rpx;
  width: 14rpx;
}
.stationBox .tabMain .tabItem:first-child+.tabItem .item_view .left_icon{
  width: 28rpx;
  height:28rpx;
}
.stationBox .tabMain .tabItem .item_view text{
  padding: 0 30rpx 0 10rpx;
}
.stationBox .tabMain .tabItem:first-child .item_view{
  border-right: 1px solid #ccc;
}
.stationBox .tabMain .tabItem.active{
  color:#2c7bfe;
}
.stationMain{
  /* padding: 30rpx; */
  height: 1100rpx;
  position: relative;
  box-sizing: border-box;
  overflow: hidden;
  box-shadow: 0 -6px 6px -5px rgba(204, 204, 204, 0.3);
}
.stationMain .sMain_item{
  width: 100%;
  height: 860rpx;
  display: none;
  position: relative;
}
.xk_box{
  position: relative;
  width: 100%;
  height: 620rpx;
  overflow: hidden;
  background-color: #011327;
}
.stationMain .sMain_item .xingKong{
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  width: 1500rpx;
  height: 1000rpx; 
  display: block;
  margin: 0 auto;
  display: block;
}
.stationMain .sMain_item .star_line{
  position: absolute;
  left:50%;
  top:50%;
  z-index: 5;
  width: 80%;
  height:210rpx;
  transform: translate(-50%,-105%);
}
.stationMain .sMain_item .star_animted{
  position: absolute;
  left:0;
  top:0;
  right:0;
  z-index: 10;
  color:#fff;
  width: 100%;
  height: 560rpx;
  margin:0 auto; 
}
.star_img{
  width: 100%;
  height:100%;
  position: absolute;
  top:0;
  left: 0;
  z-index: 30;
  transform-style: preserve-3d;
  perspective: 300px; 
  /* transform: perspective(300px) rotateX(0deg) rotateY(0deg); */
}
.star_animted .star{
  width: 160rpx;
  height:160rpx;
  position: absolute;
  top:50%;
  left: 50%;
  z-index: 30;
  transform: translate(-50%,-50%);
}
.star_animted .sun{
  width: 230rpx;
  height:230rpx;
  z-index: 40;
  position: absolute;
  top:50%;
  left: 50%;
  transform: translate(-50%,-80%);
}
.star_animted .star0{ 
  transform: translate(-50%,45%);
}
.star_animted .star1{
  width: 110rpx;
  height:110rpx;
  transform: translate(-300%,0);
}
.star_animted .star2{
  width: 90rpx;
  height:90rpx;
  transform: translate(-260%,-120%);
}
.star_animted .star3{
  width: 90rpx;
  height:90rpx;
  transform: translate(180%,-120%);
}
.star_animted .star4{
  width: 110rpx;
  height:110rpx;
  transform: translate(180%,20%);
}

