.tabShow{
  display: block !important;
}
/* 收益模拟计算器 */
.incomeCounter{
  padding: 30rpx;
  overflow: hidden;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.counter_title{
  font-size: 32rpx;
  /* margin-bottom: 20rpx; */
}
.incomeCounter .counter_item{
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 28rpx;
  margin-top: 30rpx;
}
.incomeCounter .counter_item .item_text{
  text-align: left;
  width: 40%;
}
.incomeCounter .counter_item .item_text text{
  color:#999;
  font-size: 24rpx;
}
.incomeCounter .counter_item .item_input{
  width: 60%;
}
.incomeCounter .counter_item .item_input input{
  width: 100%;
  color:#2c7bfe;
  text-align: right;
  /* padding: 20rpx 0; */
}
.incomeCounter .counter_item.getIn .item_input input{
  padding: 20rpx 0;
}

.star_animted .star_swiper{
  width: 100%;
  height: 660rpx;
  position: absolute;
  top:60%;
  left:0;
  z-index: 100;
}
.star_animted .star_swiper .project_box{
  position: relative;
  width: 100%;
  margin: 0 auto;
}
.star_animted .star_swiper .project_box image{
  width: 98% !important;
  height: 540rpx;
  margin: 0 auto;
  display: block;
}
.star_animted .star_swiper swiper{
  height: 650rpx;
}
.star_animted .star_swiper .swiper_img{
  padding-top: 150rpx;
  box-sizing: border-box;
}
.star_animted .star_swiper .swiper_img{
  overflow: visible !important;
  position: relative;
}
.star_animted .star_swiper .swiper_img .project_star_img{
  width: 160rpx;
  height:160rpx;
  position: absolute;
  top:0;
  left: 50%;
  transform: translate(-50%,0%);
  z-index: 990;
}
.station_bg_icon{
  position: absolute;
  right:0;
  bottom:-40rpx;
  z-index: 1;
  width: 30%;
  height: 240rpx;
}
.station_bg_icon image{
  width: 100%;
  height: 100%;
}
.project_box .project_item{
  position: absolute;
  top:0;
  left:0;
  z-index: 20;
  width: 100%;
  height: 100%;
  color:#333;
  padding:60rpx;
  box-sizing: border-box;
}
.project_item .item_name{
  padding:0;
  
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #e5e5e5;
  font-size: 32rpx;
}
.project_item .item_name .look_details{
  display: inline-block;
  padding:26rpx;
  color:#2c7bfe;
  font-size: 28rpx;
}
.project_box .project_item .item_data{
  position: relative;
}
.project_box .project_item .item_data .item_list{
  font-size: 28rpx;
  color:#666;
  line-height: 60rpx;
}
.txt-num1{
	overflow: hidden;
	-o-text-overflow: ellipsis;
    text-overflow: ellipsis;
    white-space:nowrap;
    -webkit-box-orient: vertical;
}
.buyBtn{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 40rpx;
  font-size: 28rpx;
  color:#999;
  text-align: center;
  background-color: #e5e5e5;
  /* background-color: #2c7bfe; */
  position: absolute;
  bottom:90rpx;
  right:80rpx;
  z-index: 100;
}
.blueBtn{
  color:#fff;
  background-color: #2c7bfe;
}







