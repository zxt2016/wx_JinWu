  border-radius: 20rpx;
  background-color: #ffffff;
  margin: 160rpx 25rpx 40rpx 25rpx;
  padding: 40rpx;
}
.inviteList text{
  font-size: 24rpx;
  height: 50rpx;
  line-height: 50rpx;
}

