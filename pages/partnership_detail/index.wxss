  width: 750rpx;
  height: 210rpx;
  z-index: -10;
}
.title{
  position: absolute;
  z-index: 10;
  margin-top: 50rpx;
  margin-left: 310rpx;
}
.title text{
  width: 100%;
  height: 55rpx;
  line-height: 55rpx;
  color: #ffffff;
  font-size: 42rpx;
  font-weight: 500;
}
.line{
  border: 1rpx solid #ffffff;
  width: 90rpx;
