page{
  width:100%;
  background-color: #f8f8f8;
  position: relative;
  padding-bottom: 380rpx;
}
.introduce{
  width: 750rpx;
  height: 380rpx;
  background-color: #ffffff;
  margin-bottom: 20rpx;
  padding: 20rpx 32rpx;
  box-sizing: border-box;
}
.introduce image{
  height: 285rpx;
  width: 690rpx;
}
.introduce text{
  float: left;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 20rpx;
}
.icon_img{
  height: 60rpx;
  line-height: 60rpx;
  margin-right: 80rpx;
  float: right;

}
.icon_img image{
  width: 25rpx;
  height: 25rpx;
}
