/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.view_body{
  width: 100%;
  height: 100%;
  min-height: 100vh;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
  border-top: 1px solid #f8f8f8;
}
.float_lf {
  float: left;
  }  
.float_rt{
  float: right;
}  

.clearfix {
  overflow: hidden;
  _zoom: 1;
  }
